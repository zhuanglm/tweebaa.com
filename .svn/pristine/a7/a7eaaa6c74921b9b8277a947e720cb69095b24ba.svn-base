<script>
    jQuery(document).ready(function () {

        //Datepicker.initDatepicker();
        $(".datepicker").datepicker({
            nextText: ">",
            prevText: "<"
        });


    });

    //关闭弹出框
    $('.closeBtn,.iagree').click(function (event) {
        var obj2 = $(this).parents(".tck")
        obj2.animate({
            top: "-500px"
        },
				300, function () {
				    obj2.parents(".greybox").hide()

				});
        return false;
    });

    //隐藏弹出 框
    function hidePopup() {
        $(".popbox").hide();
    }

</script>
