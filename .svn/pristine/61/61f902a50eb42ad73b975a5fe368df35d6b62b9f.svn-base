.pay-result-tck .reset-pay:hover { width: 148px; background-color: #007fd8; color: #fff; text-decoration: none; border-color: #007fd8 }
.pay-result-tck div.ps { font-size: 12px; line-height: 24px; padding-top: 10px }
.pay-result-tck .pay-q { display: block; line-height: 35px; color: #666; padding-top: 5px; font-size: 14px }
.yellow-shareBtn { height: 24px; line-height: 24px; display: inline-block; padding: 0 10px; color: #fff; font-size: 12px; background-color: #ff8933; border-radius: 5px }


/*first add address*/

.fir-address-box { padding: 10px; background-color: #fff; float:left; width:628px; margin-bottom:40px;}
.fir-address-box h2.t { font-size: 18px; height: 42px; line-height: 42px; font-weight: bold; border-bottom: 1px solid #e1e1e1; padding-left: 18px; width:600px;}
.fir-address-box td { padding: 8px 0; vertical-align: middle; line-height: 34px }
.fir-address-box td.t { width: 165px; padding-right: 25px; text-align: right; line-height: 36px; font-size: 14px }
.fir-address-box .txt { float: left; height: 34px; line-height: 34px; border: 0; border: 1px solid #e3e4e8; width: 200px; padding: 0 10px; border-radius: 5px; font-size: 14px; color: #888; margin-right: 10px }
.fir-address-box .txt2 { float: left; height: 34px; line-height: 34px; border: 0; border: 1px solid #e3e4e8; width: 400px; padding: 0 10px; border-radius: 5px; font-size: 14px; color: #888; margin-right: 10px }
.fir-address-box .checkbox { vertical-align: middle; position: relative; top: 1px }
.fir-address-box textarea.txt { height: 100px; width: 380px; max-width: 380px }
.fir-address-box .tag_select { font-size: 14px }
.fir-address-box .send { width: 135px; height: 40px; text-align: center; line-height: 30px; border-radius: 5px; color: #fff; background-color: #007fd8; font-size:18px; border:0;}
.fir-address-box span.h { color: #f00; padding-left: 5px; line-height: 30px }
.fir-address-box .error { font-size: 12px }



/*save address*/
.your-address { font-size: 12px; }
.your-address h2.t {  float: left;_display: inline; color: #cf333e;  }
.your-address .title { height:60px; }
.your-address .saved-address{ padding: 10px; background-color: #fff; float:left; width:628px; padding-bottom:20px;margin-bottom:20px; }
.your-address .saved-address h2.t { font-size: 18px; height: 42px; line-height: 42px; font-weight: bold; border-bottom: 1px solid #e1e1e1; padding-left: 15px; width:600px;}
.your-address .saved-address p{ font-size:14px; line-height:24px; padding:0px 15px; }
.your-address .saved-address .send { width: 135px; height: 40px; text-align: center; line-height: 30px; border-radius: 5px; color: #fff; background-color: #007fd8; font-size:18px; border:0px; margin:20px 0 0 15px}

/*shipping method*/

.your-address .ship-meth { padding:10px 10px 20px 10px; background-color: #fff;  float:right; width:286px; margin-bottom:20px;}
.your-address .ship-meth h2.t { font-size: 18px; height: 42px; line-height: 42px; font-weight: bold; border-bottom: 1px solid #e1e1e1; padding-left: 18px; width:270px;}
.your-address .ship-meth p{ padding:0 15px; font-size: 18px; font-weight:bold; line-height:50px }
.your-address .ship-meth .beizhu-txt { width: 260px; background-color: #fff; border: 1px solid #ccc; height: 45px; line-height: 20px; float:left; }
.your-address .ship-meth .gotoPay { width:250px; height:32px; vertical-align:middle; }

.btm{height:140px;}
.btm ul li{ margin-bottom:20px;}
.btm .prict-total{ font-size:24px; color: #666;  text-align: right;}
.btm .checkout-btn { width: 185px; height: 40px; text-align: center; line-height: 40px; border-radius: 5px; color: #fff; background-color: #bf4547; text-decoration: none; font-size:18px; font-weight:bold; padding:8px 45px }