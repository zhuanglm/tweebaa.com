#container #overlay 
{
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0px;
    opacity: 0.3;
    background-image: -moz-linear-gradient(#555,#fff,#fff,#555);
    background-image: -webkit-gradient(linear, 0 25%, 0 100%, from(grey), color-stop(0.5, white), to(grey));
    display: none;
}
#container #overlay #winline 
{	
    width: 100%;
    height: 5px;
    background: red;	
    position: relative;
    top: 50%;
}

#reels #results
{
    display: none;
    background: white;
    border: 3px solid gold;
    height: 120px;
    width: 250px;
    left: 80px;
    top: 120px;
    position: absolute;
    font-size: x-large;
}

#results #score {
    margin: 5px;
}
