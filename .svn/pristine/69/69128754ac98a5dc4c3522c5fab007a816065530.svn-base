        } else {
            $("#txtSharePointMoney").val(pointMoney);
            //$("#labDiscount").html("-" + pointMoney);
            setTotalDiscount(pointMoney);
            //reduct total
            var a = getTotalMoney();
            var b = parseFloat(a) - parseFloat(pointMoney);
            $("#labPayMoney2").html(b.toFixed(2));
        }
    }
</script>
