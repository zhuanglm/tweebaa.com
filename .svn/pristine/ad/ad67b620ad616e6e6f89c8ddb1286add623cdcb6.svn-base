<script type="text/javascript">
    var sec = 30;
    window.setTimeout("secAdd();", 1000);
    function secAdd() {
        sec = sec - 1;
        if (sec >= 0) {
            $("#labSec").text(sec);
        }
        if (sec == 0) {
            GetVisitCount0();
        }
        window.setTimeout("secAdd();", 1000);
    }
    function GetVisitCount0() {
        var _data = "{'action':'getCount'}";
        $.ajax({
            type: "POST",
            url: '/AjaxPages/UserActivity.aspx',
            data: _data,
            success: function (resu) {
                $("#labView").text(resu);
            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
            }
        });

    }
    function ShowEventDetails() {
        var sVal = $("#btnDetails").val();
        if (sVal == "DETAILS" || sVal=="") {
            //show div
            $("#btnDetails").val("Hide Details");
            $("#btnDetails").html("HIDE DETAILS");
            $("#TweebotEventShare").show();
        } else {
            $("#TweebotEventShare").hide();
            $("#btnDetails").val("DETAILS");
            $("#btnDetails").html("DETAILS");
        }
    }
</script>
