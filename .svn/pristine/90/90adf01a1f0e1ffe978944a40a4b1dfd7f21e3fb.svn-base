            <!--16-->
                  <div class="panel panel-default">
                        <div class="panel-heading">
                            <h4 class="panel-title">
                                <a class="accordion-toggle collapsed" data-toggle="collapse" data-parent="#accordion" href="#collapse16">
                          Can I send an email using TweeBot?
                                </a>
                            </h4>
                        </div>
