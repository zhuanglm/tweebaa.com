using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

namespace TweebaaWebApp2.ShipAPI
{
    public class FosdickShipmentDetail
    {
        public string sFosdickOrderNo { set; get; }             // Fosdick order number
        public int iFosdickLineNo {set; get;}                   // Fosdick line item number
        public string sSku { set; get; }                        // Fosdick sku
        public int iQty {set; get;}                             // Quantity shipped
        public string sExternalOrderNo { set; get; }            // External order number 
        public int iExternalLineNo { set; get; }                // External order line item number 
        public string sExternalSku { set; get; }                // External sku
        public DateTime dtShipDate { set; get; }                // Date shipped
        public List<FosdickShipmentTracking> trackingList;      // list of tracking information
    }
}