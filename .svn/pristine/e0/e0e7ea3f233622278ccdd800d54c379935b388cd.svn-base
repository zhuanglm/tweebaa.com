/*shopping reward*/

.shopdiv {
	margin:20px;padding:0px;
	width:600px;
	border:1px solid #000000;
	
}.shopdiv table{
    border-collapse: collapse;
        border-spacing: 0;
	width:100%;
	height:100%;
	margin:0px;padding:0px;
}

.shopdiv tr:nth-child(odd){ background-color:#e5e5e5; }
.shopdiv tr:nth-child(even)    { background-color:#ffffff; }
.shopdiv td{
	vertical-align:middle;
	border:1px solid #000000;
	border-width:0px 1px 1px 0px;
	text-align:center;
	padding:3px;
	color:#000000;
}.shopdiv tr:last-child td{
	border-width:0px 1px 0px 0px;
}.shopdiv tr td:last-child{
	border-width:0px 0px 1px 0px;
}.shopdiv tr:last-child td:last-child{
	border-width:0px 0px 0px 0px;
}
.shopdiv tr:first-child td{
		background:-o-linear-gradient(bottom, #cccccc 5%, #cccccc 100%);	background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #cccccc), color-stop(1, #cccccc) );
	background:-moz-linear-gradient( center top, #cccccc 5%, #cccccc 100% );
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr="#cccccc", endColorstr="#cccccc");	background: -o-linear-gradient(top,#cccccc,cccccc);

	background-color:#cccccc;
	border:0px solid #000000;
	text-align:center;
	border-width:0px 0px 1px 1px;
	font-size:14px;
	font-family:Arial;
	font-weight:bold;
	color:#000000;
}
.shopdiv tr:first-child:hover td{
	background:-o-linear-gradient(bottom, #cccccc 5%, #cccccc 100%);	background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #cccccc), color-stop(1, #cccccc) );
	background:-moz-linear-gradient( center top, #cccccc 5%, #cccccc 100% );
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr="#cccccc", endColorstr="#cccccc");	background: -o-linear-gradient(top,#cccccc,cccccc);

	background-color:#cccccc;
}
.shopdiv tr:first-child td:first-child{
	border-width:0px 0px 1px 0px;
}
.shopdiv tr:first-child td:last-child{
	border-width:0px 0px 1px 1px;
}
