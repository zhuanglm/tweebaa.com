/*浮动样式*/
.sideNav {width:63px;min-height:600px;position:fixed;z-index:44;right:15px;top:80px;z-index:999;}
.sideNav.open {width:230px;}
.sideNav em {width:31px;height:21px;display:block;position:absolute;top:-21px;left:-5px;cursor:pointer;}
.sideNav em.op {background:url(../college/images/college-navico2.png) no-repeat;}
.sideNav em.cl {background:url(../college/images/college-navico1.png);display:none;}
.sideNav #gotop,.sideNav #opNav,.sideNav #clNav {border:1px solid #666666;color: #666666;width: 63px;display: block;height:45px;text-align: center;text-decoration: none;line-height:18px;padding-top:42px;margin-bottom:15px;background-color: #fff!important;}
.sideNav #gotop:hover,.sideNav #opNav:hover {border:1px solid #662d91;color: #662d91}
.sideNav #gotop {background:url(../college/images/gotop.png) center 10px no-repeat;cursor:pointer;}
.sideNav #gotop:hover {margin-bottom:15px;background: url(../college/images/gotop2.png) center 10px no-repeat;}
.sideNav #opNav,.sideNav #clNav {margin-bottom:15px;padding-top:47px;border:1px solid #662d91;color:#662d91;cursor:pointer;}
.sideNav #opNav {background:url(../college/images/college-open.png) center 10px no-repeat;}
.sideNav #clNav {background: url(../college/images/college-close.png) center 10px no-repeat;display:none;}
.sideNav.open #opNav {display:none;}
.sideNav.open #clNav {display:block;}

.sideNavList {margin-bottom:-1px;width:228px;padding:10px 0;border:1px solid #662d91;color:#662d91;background:#fff;display:none;zoom:1;}
.sideNav.open .sideNavList {display:block;}
.sideNavList dl {margin:0 auto;width:190px;}
.sideNavList dt {padding:5px 0;position:relative;}
.sideNavList dt a {padding-left:20px;font-weight:bold;word-break:break-all;line-height:18px;display:block;}
.sideNavList dt i {width:20px;height:14px;background:url(../college/images/college-navico3.png);display:none;position:absolute;top:8px;left:-25px;}
.sideNavList dl.cur dt i {display:block;}
.sideNavList dt span {width:14px;height:14px;display:block;background:url(../college/images/college-navico4.png);position:absolute;top:8px;left:0;cursor:pointer}
.sideNavList dl.on dt span {background:url(../college/images/college-navico5.png);}
.sideNavList dt a {color:#000;}
.sideNav .clr-1 {color:#662d91;}
.sideNav .clr-2 {color:#f68e56;}
.sideNav .clr-3 {color:#39cdab;}
.sideNav .clr-4 {color:#bf4547;}
.sideNav .clr-5 {color:#09f;}
.sideNavList dl dd {padding-left:20px;display:none;}
.sideNavList dl.on dd {display:block;}
.sideNavList dd a {padding:5px 0;font-size:14px;color:#666;line-height:18px;display:block;}
.sideNavList dd a:hover {color:#662d91;}

