using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

namespace TweebaaWebApp.ShipAPI
{
    public class EshipperQuoteReply
    {
        private int _carrierId;
        private string _carrierName;
        private int _serviceId;
        private string _serviceName;
        private string _modeTransport;
        private int _transitDays;
        private decimal _baseCharge;
        private decimal _fuelSurcharge;
        private decimal _totalCharge;
        private string _currency;

        public int CarrierId
        {
            set { _carrierId = value; }
            get { return _carrierId; }
        }
        public string CarrierName
        {
            set { _carrierName = value; }
            get { return _carrierName; }
        }
        public int ServiceId
        {
            set { _serviceId = value; }
            get { return _serviceId; }
        }
        public string ServiceName
        {
            set { _serviceName = value; }
            get { return _serviceName; }
        }
        public string ModeTransport
        {
            set { _modeTransport = value; }
            get { return _modeTransport; }
        }
        public int TransitDays
        {
            set { _transitDays = value; }
            get { return _transitDays; }
        }

        public decimal BaseCharge
        {
            set { _baseCharge = value; }
            get { return _baseCharge; }
        }

        public decimal FuelSurcharge
        {
            set { _fuelSurcharge = value; }
            get { return _fuelSurcharge; }
        }

        public decimal TotalCharge
        {
            set { _totalCharge = value; }
            get { return _totalCharge; }
        }

        public string Currency
        {
            set { _currency = value; }
            get { return _currency; }
        }

    }
}