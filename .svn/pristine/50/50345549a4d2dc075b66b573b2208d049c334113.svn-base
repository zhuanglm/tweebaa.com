}

@media (max-width: 1200px) {
    .header-v5 .mega-menu .dropdown.mega-menu-fullwidth .dropdown-menu {
        width: auto;
    }

    .header-v5 .mega-menu .mega-menu-content {
        padding-right: 0;
    }

    .header-v5 .mega-menu .mega-menu-content [class^="col-"] {
        padding-right: 25px;
