                    function qiandaoFuc() {
                        var userid = $("#hidQianDaoUserid").val();
                        if (userid != "") {
                            var res = TweebaaWebApp.Home.Index.SaveQianDao(userid).value;
                            if (res == "error") {
                                alert("Sign in failed");
                            } else if (res == "Haved") {
